result-view {
   match {
     Chooser (chooser)
   }
   render {
     layout {
       section {
         content {
           single-line {
             text {
               style (Title_S)
               value {
                 template-macro (CHOOSE_MESSAGE)
               }
             }
           }
           image {
             url {
               template ("images/chooser.png")
             }
           }
         }
       }
     }
   }
 }
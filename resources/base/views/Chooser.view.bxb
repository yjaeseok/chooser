result-view {
   match {
     Chooser (chooser)
   }
   render {
     layout {
       section {
         content {
           single-line {
             text {
               style (Title_M)
               value ("Total Player : #{value(chooser.numPlayer)}")
             }
           }
           single-line {
             text {
               style (Title_S)
               value ("Selected Player: #{value(chooser.currentPlayer)}")
             }
           }
         }
       }
     }
   }
 }